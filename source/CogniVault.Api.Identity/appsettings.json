{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "JwtOptions": {
    "ClockSkew": 5,
    "Issuers": ["Issuer1", "Issuer2"],
    "CertificateThumbprint": "YourCertificateThumbprintHere",
    "SecretKey": "YourSecretKeyForProduction",
    "Algorithm": "HS256",
    "ValidAudiences": ["Audience1", "Audience2"],
    "ExpiryDurationInMinutes": 60,
    "RequireHttpsMetadata": true,
    "SaveToken": true,
    "NameClaimType": "name",
    "RoleClaimType": "role"
  },
  "AllowedHosts": "*"
}
